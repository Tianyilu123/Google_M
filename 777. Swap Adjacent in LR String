class Solution {
    public boolean canTransform(String start, String end) {
        int r = 0, l = 0;
        for (int i = 0; i< start.length(); i++){
            if (start.charAt(i) == 'R'){ r++; l = 0;}
            if (end.charAt(i) == 'R') { r--; l = 0;}
            if (end.charAt(i) == 'L') { l++; r = 0;}
            if (start.charAt(i) == 'L') { l--; r = 0;}
            if (l < 0 || r < 0) return false;
        }

        if (l != 0 || r != 0) return false;
        return true;
    }
}
