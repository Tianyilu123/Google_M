class Solution {
    public boolean canJump(int[] nums) {
        if(nums.length < 2) return true;
        int reach = 0;
        for(int i=0; i<nums.length && reach>=i; i++) {
            reach = Math.max(reach, nums[i]+i);
            if(reach >= nums.length-1) return true;
        }
        return false;
    }
}
